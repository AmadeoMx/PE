dnu
doesNotUnderstand: aMessage
	| methodName |
	(fileList respondsTo: aMessage selector)
		ifTrue: [
methodName := aMessage createStubMethod allButLast: 24.
self class compileSilently: methodName , '^ fileList ', methodName classified: #autogenerated.
		^aMessage sentTo: self]
		ifFalse: [super doesNotUnderstand: aMessage].